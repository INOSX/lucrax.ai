{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index_simple.py"
    },
    {
      "src": "/api",
      "dest": "/api/index_simple.py"
    },
    {
      "src": "/",
      "dest": "/api/dash.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/dash.py"
    }
  ]
}
